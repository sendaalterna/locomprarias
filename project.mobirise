                "@media (max-width: 992px)": {
                  "height": "300px"
                }
              },
              "align-items": "center"
            },
            ".item-box-img": {
              "padding": "0"
            },
            "@media (min-width: 1400px)": {
              ".container-fluid": {
                "padding": "0 2rem"
              }
            },
            ".item-box": {
              "background": "#efefef",
              "justify-content": "center",
              "width": "100%",
              "overflow": "auto",
              "@media (min-width: 992px)": {
                "max-width": "1000px"
              },
              "@media (max-width: 991px)": {
                "width": "90%",
                "margin": "1rem",
                "padding-top": "3rem"
              }
            },
            ".close:before, .close:after": {
              "background-color": "#777"
            },
            ".item-wrapper": {
              "cursor": "pointer",
              ".btn": {
                "transform": "translateY(4rem)",
                "transition": "all 0.3s",
                "@media (max-width: 767px)": {
                  "padding": "1.2rem 3rem"
                }
              }
            },
            ".item:focus, span:focus": {
              "outline": "none"
            },
            ".item": {
              ":hover": {
                "cursor": "pointer"
              },
              ".item-box-text, .cur, .item-box-price": {
                "&:hover": {
                  "cursor": "default"
                }
              },
              "&:hover": {
                ".btn": {
                  "transform": "translateY(0rem)"
                }
              }
            },
            ".item-wrapper, .item-box, .item-img": {
              "position": "relative"
            },
            ".item-btn, .item-box-btn": {
              "margin": "0"
            },
            ".item-box-title": {
              "color": "#000"
            },
            ".item-box-text": {
              "color": "#999999"
            },
            ".amp-shop__title": {
              "text-align": "center"
            },
            ".item-box-wrapper": {
              "background-color": "#efefef"
            },
            "& when (@customStyles)": {
              "& when (@addPadding)": {
                "& when not (@imgPadding)": {
                  ".item-wrapper": {
                    "padding": "(@yPadding * 1rem) (@xPadding * 1rem)"
                  }
                },
                "& when (@imgPadding)": {
                  ".item-img": {
                    "padding-bottom": "0"
                  },
                  ".item-content": {
                    "padding": "(@yPadding * 1rem) (@xPadding * 1rem)"
                  }
                }
              },
              "& when (@boxShadow)": {
                ".item-wrapper": {
                  "box-shadow": "0px 0px (@shadowSize * 1px) 0px rgba(red(@shadowColor), green(@shadowColor), blue(@shadowColor), @shadowOpacity)"
                }
              },
              "& when (@useColorCard)": {
                ".item-wrapper": {
                  "position": "relative",
                  "z-index": "0",
                  "&:after": {
                    "content": "\"\"",
                    "display": "block",
                    "position": "absolute",
                    "top": "0",
                    "bottom": "0",
                    "left": "0",
                    "right": "0",
                    "opacity": "@opacityCard",
                    "background": "@colorCard",
                    "transition": "opacity .3s",
                    "pointer-events": "none",
                    "z-index": "-1"
                  }
                }
              },
              "& when (@useBorderRadius)": {
                ".item-wrapper": {
                  "z-index": "12",
                  "border-radius": "(@borderRadius * 1px)",
                  "overflow": "hidden"
                }
              },
              ".item-wrapper": {
                "color": "@fontColor"
              },
              ".btn": {
                "padding": "1.2rem 4rem",
                "background-color": "transparent !important",
                "border-color": "@btnColor !important",
                "&:before, &:after": {
                  "background": "@btnColor !important"
                },
                "&:hover, &:focus, &:active": {
                  "color": "@btnColor !important",
                  "border-color": "@btnColor !important"
                }
              }
            },
            ".item-content": {
              "overflow": "hidden"
            },
            ".item-box-text1": {
              "color": "#757575"
            }
